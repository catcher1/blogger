<h1>All Tags</h1>

<ul id="tags">
	<% @tags.each do |tag| %>
		<li>
			<%= link_to tag.name, tag_path(tag), 							
			class:'tag_name', id: "tag_#{tag.id}" %>
		</li>
		<% if logged_in? %>
		<li>
			<%= link_to 'delete tag', tag_path(tag), method: :delete, data: {confirm:'Really delete the tag?'}%>
		</li>
				<% end %>
	<% end %>
	
	<br>
</ul>